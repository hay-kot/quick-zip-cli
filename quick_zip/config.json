{
    "config": {
        "enable_webhooks": false,
        "webhook_address": "https://webhooks.com/webhook"
    },
    "vars": {
        "DATA_DIR": "./data"
    },
    "defaults": {
        "name": "ENTRY",
        "source": "",
        "destination": "/backups",
        "clean_up": false,
        "clean_up_source": false,
        "keep": 1,
        "audit": true,
        "oldest": 0
    },
    "jobs": [
        {
            "name": "Dev Backup",
            "source": "${DATA_DIR}/src",
            "destination": "${DATA_DIR}/dest",
            "clean_up": true,
            "clean_up_source": true
        },
        {
            "name": "Dev Backup 2",
            "source": "${DATA_DIR}/src",
            "destination": "${DATA_DIR}/dest",
            "clean_up": true,
            "clean_up_source": true
        },
        {
            "name": "Dev Backup 3",
            "source": "${DATA_DIR}/src",
            "destination": "${DATA_DIR}/dest",
            "clean_up": true,
            "clean_up_source": true
        }
    ]
}