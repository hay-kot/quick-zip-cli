{
    "config": {
        "enable_webhooks": true,
        "webhook_address": "https://webhooks.com/webhook"
    },
    "vars": {
        "VAR_1": "var_1_value",
        "VAR_2": "var_2_value"
    },
    "defaults": {
        "name": "ENTRY",
        "source": "",
        "destination": "./nas/backups",
        "clean_up": false,
        "keep": 4
    },
    "jobs": [
        {
            "name": "${VAR_1}",
            "source": "/${VAR_2}/entry_1/${VAR_2}",
            "destination": "/home/entry_1/${VAR_2}",
            "clean_up": false
        },
        {
            "name": "entry_2",
            "source": "/home/entry_2",
            "destination": "/home/entry_2/backups",
            "clean_up": false
        }
    ]
}